<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:background="@color/black"
    tools:context=".activity.FamilyAnchorDetailActivity">

    <include

        android:id="@+id/include4"
        layout="@layout/include_top_toolbar"></include>


    <ImageView

        android:id="@+id/imageView5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/bg_anchor_manager_detail"
        app:layout_constraintBottom_toBottomOf="@+id/textView25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include4" />

    <ImageView
        android:id="@+id/iv_head"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include4" />

    <TextView
        android:id="@+id/tv_head"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:text="#FF363636"
        android:textColor="#FF363636"
        app:layout_constraintEnd_toEndOf="@+id/iv_head"
        app:layout_constraintStart_toStartOf="@+id/iv_head"
        app:layout_constraintTop_toBottomOf="@+id/iv_head" />

    <TextView
        android:id="@+id/tv_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:text="#FF818181"
        android:textColor="#FF818181"
        app:layout_constraintEnd_toEndOf="@+id/tv_head"
        app:layout_constraintStart_toStartOf="@+id/tv_head"
        app:layout_constraintTop_toBottomOf="@+id/tv_head" />


    <ImageView
        android:src="#FF979797"
        android:id="@+id/imageView3"
        android:layout_width="1dp"
        android:layout_height="34dp"
        android:layout_marginTop="31dp"
        app:layout_constraintEnd_toEndOf="@+id/tv_content"
        app:layout_constraintStart_toStartOf="@+id/tv_content"
        app:layout_constraintTop_toBottomOf="@+id/tv_content" />

    <TextView
        android:id="@+id/tv_total_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginEnd="8dp"
        android:includeFontPadding="false"
        android:text="123小时45分34秒 "
        android:textColor="#FF424242"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:includeFontPadding="false"
        android:paddingBottom="22dp"
        android:text="累计直播"
        app:layout_constraintEnd_toEndOf="@+id/tv_total_time"
        app:layout_constraintStart_toStartOf="@+id/tv_total_time"
        app:layout_constraintTop_toBottomOf="@+id/tv_total_time" />

    <TextView
        android:id="@+id/tv_total_income"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:includeFontPadding="false"
        android:text="5634354豆 "
        android:textColor="#FF424242"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:includeFontPadding="false"
        android:paddingBottom="22dp"
        android:text="累计收益"
        app:layout_constraintEnd_toEndOf="@+id/tv_total_income"
        app:layout_constraintStart_toStartOf="@+id/tv_total_income"
        app:layout_constraintTop_toBottomOf="@+id/tv_total_income" />



    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:background="@drawable/bg_family_manager_top"
        android:padding="17dp"
        app:layout_constraintEnd_toEndOf="@id/imageView5"
        app:layout_constraintStart_toStartOf="@id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5">

        <ImageView
            android:id="@+id/iv_choose_time"
            app:layout_constraintTop_toTopOf="@id/tv_start_time"
            android:paddingEnd="10dp"
            android:paddingBottom="10dp"
            android:paddingStart="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:src="@drawable/ic_choose_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/tv_start_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2019-08-22"
            android:textColor="#FF363636"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="至"
            android:textColor="#FF363636"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@id/tv_start_time"
            app:layout_constraintTop_toTopOf="@+id/tv_start_time" />

        <TextView
            android:id="@+id/tv_end_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2019-08-22"
            android:textColor="#FF363636"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@id/tv1"
            app:layout_constraintTop_toTopOf="@+id/tv_start_time" />

        <TextView
            android:id="@+id/tv_during_total_time"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="时长总计：35:45:23"
            android:textColor="#FF818181"
            android:textSize="13sp"
            app:layout_constraintEnd_toStartOf="@id/tv_during_total_income"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintStart_toStartOf="@id/tv_start_time"
            app:layout_constraintTop_toBottomOf="@+id/tv_start_time" />

        <TextView
            android:id="@+id/tv_during_total_income"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="收益总计：343.09万"
            android:textColor="#FF818181"
            android:textSize="13sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintStart_toEndOf="@id/tv_during_total_time"
            app:layout_constraintTop_toBottomOf="@+id/tv_start_time" />






        <TextView
            android:id="@+id/tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="17dp"
            android:text="每日明细"
            android:textColor="#FF363636"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="@id/tv_start_time"
            app:layout_constraintTop_toBottomOf="@+id/tv_during_total_income" />

        <ImageView
            android:id="@+id/tv_today_more"
            android:paddingEnd="10dp"
            android:paddingBottom="10dp"
            android:paddingStart="10dp"
            app:layout_constraintTop_toTopOf="@id/tv2"
            app:layout_constraintEnd_toEndOf="@id/iv_choose_time"
            android:src="@drawable/ic_next_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/tv_today_total_time"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="时长：35:45:23"
            android:textColor="#FF818181"
            android:textSize="13sp"
            app:layout_constraintEnd_toStartOf="@id/tv_today_total_income"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintStart_toStartOf="@id/tv_start_time"
            app:layout_constraintTop_toBottomOf="@+id/tv2" />

        <TextView
            android:id="@+id/tv_today_total_income"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="收益：345.24万"
            android:textColor="#FF818181"
            android:textSize="13sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintStart_toEndOf="@id/tv_today_total_time"
            app:layout_constraintTop_toBottomOf="@+id/tv2" />





    </android.support.constraint.ConstraintLayout>



</android.support.constraint.ConstraintLayout>